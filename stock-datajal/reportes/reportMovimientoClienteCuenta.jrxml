	<queryString>
		<![CDATA[SELECT car_numero , car.fecha as car_fecha1, car.hora, car.nro_boleta, car.id_cliente, cli.nombre, cli.celular, cli.direccion, car.tipoentidad,  car.monto as car_monto1, 
car_proceso, car_situacion, ica_cuenta, ica_secuencia, ica_dias, ica_vencimiento as ica_vencimiento1, 
ica_monto as ica_monto1, ica_iva, ica_documento, ica_situacion
,coalesce((select SUM(icl_monto) FROM item_cobro_clientes where icl_secuencia_cuenta = icr.ica_secuencia), 0) AS pagado,
('Ventas de productos #') AS nombre_ingreso 
 FROM cuenta_a_recibir car, item_cuenta_a_recibir ICR, clientes cli 
	  WHERE car_numero=ica_cuenta AND car_situacion=0  and ica_situacion = 0 and car.id_cliente=cli.id 
	  $P!{fechaFiltro1}
	  $P!{idCliente}
 UNION 
SELECT car_numero, MAX(car.fecha) AS car_fecha1, car.hora, '' AS nro_boleta, car.id_cliente, cli.nombre, cli.celular, cli.direccion, car.tipoentidad,  SUM(car.monto) AS car_monto1, 
car_proceso,  car_situacion, ica_cuenta, ica_secuencia, ica_dias, MAX(ica_vencimiento) AS ica_vencimiento1, 
SUM(ica_monto) AS ica_monto1, ica_iva, '' AS ica_documento, ica_situacion, 
COALESCE((SELECT SUM(icl_monto) AS pagado FROM item_cobro_clientes where icl_secuencia_cuenta = icr.ica_secuencia), 0) AS pagado, 
   'Saldo Anterior' AS nombre_ingreso 
 FROM cuenta_a_recibir car, item_cuenta_a_recibir ICR , clientes cli
	  WHERE car_numero=ica_cuenta and car_situacion=0  and ica_situacion = 0  and car.id_cliente=cli.id 
	  $P!{fechaFiltro2}	  
	  $P!{idCliente} 
GROUP BY car.ID_CLIENTE, cli.nombre, cli.celular, cli.direccion, car.tipoentidad,car_numero, car.hora, ICA_iva, car_proceso, car_situacion,  ica_cuenta, ica_secuencia, ica_dias, ica_iva, ica_situacion  
$P!{orden} ]]>
	</queryString>
